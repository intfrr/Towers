<?xml version="1.0" encoding="UTF-8"?>
<tns:mainConfig xmlns:tns="http://lineup/util/config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://lineup/util/config ../xsd/lineup.xsd ">
	
	<tns:levels>
		<tns:level sequence="1">
			<tns:base sequence="0">
				<tns:x>102</tns:x>
				<tns:y>118</tns:y>
			</tns:base>
			<tns:route>
				<tns:waypoint sequence="1">
					<tns:x>9</tns:x>
					<tns:y>176</tns:y>
				</tns:waypoint>
				<tns:waypoint sequence="2">
					<tns:x>175</tns:x>
					<tns:y>141</tns:y>
				</tns:waypoint>
				<tns:waypoint sequence="3">
					<tns:x>133</tns:x>
					<tns:y>29</tns:y>
				</tns:waypoint>
				<tns:waypoint sequence="4">
					<tns:x>35</tns:x>
					<tns:y>57</tns:y>
				</tns:waypoint>
			</tns:route>
			<tns:background>lvl1.jpg</tns:background>
			<tns:waves>
				<tns:wave sequence="1">
					<tns:creepRef>smallspider</tns:creepRef>
					<tns:interval>4</tns:interval>
					<tns:count>8</tns:count>
				</tns:wave>
				<tns:wave sequence="2">
					<tns:creepRef>blue</tns:creepRef>
					<tns:interval>3</tns:interval>
					<tns:count>10</tns:count>
				</tns:wave>
				<tns:wave sequence="3">
					<tns:creepRef>red</tns:creepRef>
					<tns:interval>4</tns:interval>
					<tns:count>12</tns:count>
				</tns:wave>
				<tns:wave sequence="4">
					<tns:creepRef>yellow</tns:creepRef>
					<tns:interval>0.3</tns:interval>
					<tns:count>30</tns:count>
				</tns:wave>
				<tns:wave sequence="5">
					<tns:creepRef>bg</tns:creepRef>
					<tns:interval>10</tns:interval>
					<tns:count>3</tns:count>
				</tns:wave>
				<tns:wave sequence="6">
					<tns:creepRef>mothership</tns:creepRef>
					<tns:interval>10</tns:interval>
					<tns:count>1</tns:count>
				</tns:wave>
			</tns:waves>
		</tns:level>
	</tns:levels>
	
	<tns:creeps>
		<tns:creep ref="smallspider">
			<tns:fqName>lineup.model.implementations.creeps.SmallSpider</tns:fqName>
		</tns:creep>
		<tns:creep ref="blue">
			<tns:fqName>lineup.model.implementations.creeps.BlueCreep</tns:fqName>
		</tns:creep>
		<tns:creep ref="red">
			<tns:fqName>lineup.model.implementations.creeps.RedCreep</tns:fqName>
		</tns:creep>
		<tns:creep ref="yellow">
			<tns:fqName>lineup.model.implementations.creeps.YellowCreep</tns:fqName>
		</tns:creep>
		<tns:creep ref="bg">
			<tns:fqName>lineup.model.implementations.creeps.BossGreen</tns:fqName>
		</tns:creep>
		<tns:creep ref="mothership">
			<tns:fqName>lineup.model.implementations.creeps.Mothership</tns:fqName>
		</tns:creep>
	</tns:creeps>
	
</tns:mainConfig>
